in:
  type: file
  path_prefix: "example_map.json"
  parser:
    type: jsonl
    columns:
      - {name: "key", type: "string"}
      - {name: "data", type: "json"}

out:
  type: s3_per_record
  data_columns: [data]
  mode: single_column
  bucket: {{ env.S3_BUCKET }}
  key: "${key}"
  serializer: msgpack
